#Example of class
class baseemployee:
    id = 0
    name="Aniruddha"
    def display(self):
        print(self.name,self.id)
baseemployee().display()
#Example of class object
class Employee:
    def __init__(self, name, salary):
        self.name = name
        self.salary = salary
    
    def displayemp(self):
        print("Name:", self.name)
        print("Salary:", self.salary)

emp1 = Employee("Krinapal", 50000000)

print(emp1.displayemp())

